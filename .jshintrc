{
  "asi":true, // 允许省略分号
  "boss":false,// 禁止在if、for、while中使用赋值语句
  "bitwise":true,//禁用位运算符(如^，	，&) 出现&也很可能是想写&&
  "camelcase":false,//使用驼峰命名(camelCase)或全大写下划线命名(UPPER_CASE)
  "curly":false, //（不用必须） if和while等语句中使用{}来明确代码块 
  "laxbreak":true,//允许不安全的行中断(与laxcomma配合使用)
  "laxcomma":true,//允许逗号开头的编码样式
  "regexp":true,
  "browser": true,
  "loopfunc":true,//允许循环中定义函数
  "expr":true,//允许应该出现赋值或函数调用的地方使用表达式
  "evil":false,//禁止使用eval
  "eqeqeq": false, //不用必须 使用 === 或者 !==
  "eqnull": false, //允许==null
  "esnext":true,//允许es6
  "funcscope":false,//允许在代码块{ }内定义变量而在外部使用 ，例如在if中定义，而在外部使用
  "freeze":false,//禁止复写原生对象(如Array, Date)的原型
  "forin":true,//在for in循环中要求使用Object.prototype.hasOwnProperty()来过滤原型链中的属性
  //   "es5": true,//强制使用es5来检查
  // "indent":2,"//代码块缩进宽度"
  "globals":{ //可以使用的全局变量，忽略变量未定义错误
      "$":true,
      "IScroll":true,
      "ga":true,
      "alert":true
  },
  "immed": true,//匿名函数调用必须(function() {// body }());而不是(function() {// body})();
  "latedef": false, //变量定义前禁止使用
  "lastsemic":true,//允许单行控制块省略分号 var name = (function() { return 'Anton' }());
  "maxerr":50,//JSHint中断扫描前允许的最大错误数
  // "maxlen":100, //一行中最大字符数
  //"maxstatement":20,//函数中最大语句数,鼓励一个函数中不要写太多语句
  "maxparams":5,//函数可以接收的最大参数数量 
  "node": true, 
  "newcap": true,  //构造函数首字母必须大写
  "noarg": true,   //禁止使用arguments.caller和arguments.callee
  "nonew":false, //禁止使用构造器 new MyConstructor();构造一个对象，却不给它赋值到某个变量   
  "noempty":true, //禁止出现空代码块
  "plusplus":false, //禁止使用++和–
  // "quotemark":false,//统一使用单引号或双引号
  "proto": true,//允许 proto  不是所有的浏览器都支持__proto__.
  "undef": true,  // 所有的非全局变量，在使用前必须都被声明
  "unused": true, // 禁止定义变量却不使用
  "strict": false, //强制使用ES5的严格模式
  "supernew":true, //允许new function() {…}和new Object;
  "smarttabs": true,//允许混合tab和space排版
  "shadow":false,// 不允许同一作用域下多次定义
  "sub":true      //允许允许person[‘name’]
}